@model StutorDataObjects.SubjectRequest

@{
    ViewBag.Title = "DeleteSubjectRequest";
}

<h2>Delete Request</h2>

<h3>Are you sure you want to delete this subject request?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Subject Area
        </dt>

        <dd>
            @Html.DisplayFor(model => model.subjectAreaName)
        </dd>

        <dt>
            Subject Name
        </dt>

        <dd>
            @Html.DisplayFor(model => model.subjectName)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-default" /> |
            @Html.ActionLink("Back to List", "Index")
        </div>
    }
</div>
